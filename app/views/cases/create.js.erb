<% if @case.errors.empty? %>
$('#allcases').html("<%= escape_javascript(render :partial => "cases/c", :collection => @quote.cases.order("created_at DESC")) %>")
$('#myModal').html("<%= escape_javascript(render 'quotes/form1') %>");  
$('#myModal').modal('hide');
<% else %>
alert("<%= @case.errors.full_messages.join('\n')%>")
<% end %>

